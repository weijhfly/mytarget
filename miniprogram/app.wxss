/**app.wxss**/
page {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.page{
  width: 100%;
  height: 100%;
  background: #f6f6f6;
  padding-top: 120rpx;
}
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 
view{
  -webkit-box-sizing: border-box;
     -moz-box-sizing: border-box;
          box-sizing: border-box;
}
.fixed-center{
	position: fixed;
	left: 50%;
	top: 50%;
  z-index: 100;
	-webkit-transform: translateX(-50%) translateY(-50%);
	-moz-transform: translateX(-50%) translateY(-50%);
	-ms-transform: translateX(-50%) translateY(-50%);
	transform: translateX(-50%) translateY(-50%);
}
.wait-img{
  width: 124rpx;
  height:124rpx;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.bColor{
  background: #fff;
}
.w100{
  width: 100%;
}
.h40{
  height: 40rpx;
}
.header{
  position: fixed;
  left: 0;
  top: 0;
  height: 120rpx;
  padding-top: 50rpx;
  width: 100%;
  text-align: center;
  font-size: 40rpx;
}
.head-return{
  position: absolute;
  left: 0;
  top: 50rpx;
  width: 70rpx;
  height: 70rpx;
}
.footer{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  padding:5rpx 0 5rpx;
  border-top: 1rpx solid #acacac;
  text-align: center;
  font-size: 26rpx;
}
.footer .footer-item{
  float: left;
  width: 33.333333%;
}
.footer .footer-item view{
  margin-top: -10rpx;
}
.footer image{
  width: 60rpx;
  height: 60rpx;
}
.footer .active{
  color: #e92a2a;
}
wux-cell-group{
  width: 100%;
}
.list .wux-cell{
  padding:25rpx 30rpx;
}
.page .wux-fab-button__label{
  background: none;
  opacity: 1;
}
.page .wux-fab-button__button{
  left: -40rpx;
}
.page .wux-fab-button__button +.wux-fab-button__button{
  left:-60rpx;
  top:-20rpx;
}
.share-canvas{
 position: absolute;
 top: 0;
 left: -999px;
}
.page .wux-fab-button__action{
  right: 20rpx;
  bottom: 20rpx;
}
